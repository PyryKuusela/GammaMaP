 RowBox[{
  RowBox[{"gAddAssumptions", "[", 
   RowBox[{"F", "\[Element]", 
    RowBox[{
     RowBox[{"g\[CapitalOmega]", "[", 
      RowBox[{"2", ",", "0", ",", "0"}], "]"}], "[", "]"}]}], "]"}], 
  ";"}]}], "Input",
 CellChangeTimes->{{3.762078033924445*^9, 3.762078037901968*^9}},
 CellLabel->"In[65]:=",ExpressionUUID->"0b2c9958-1dcf-4127-82a8-b9d1c40d45f2"],

Cell[TextData[{
