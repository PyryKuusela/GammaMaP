Cell[BoxData[
 RowBox[{"gReIm", "[", 
  RowBox[{"gTensor", "[", 
   RowBox[{"F", ",", 
    RowBox[{"{", 
     RowBox[{"a", ",", "b"}], "}"}]}], "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.7550969160279627`*^9, 3.755096916029668*^9}},
