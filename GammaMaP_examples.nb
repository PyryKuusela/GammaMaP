Cell[TextData[{
 "gProductToBL[",
 StyleBox["expr",
  FontWeight->"Bold"],
 "]\nExpresses every bilinear in ",
 StyleBox["expr",
  FontWeight->"Bold"],
 " in the gBL notation.\n",
 StyleBox["Arguments",
  FontWeight->"Bold"],
 "\n",
 StyleBox["expr",
  FontWeight->"Bold"],
 " is the expression to be modified."
}], "Text",
