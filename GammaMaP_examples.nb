 "\[Epsilon] = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Eta]", "\[CircleTimes]", 
    SuperscriptBox["e", 
     RowBox[{"\[CapitalDelta]", "/", "2"}]]}], TraditionalForm]],
  ExpressionUUID->"f5a783f4-4872-4173-8d3a-034126a19cd8"],
 " \[Xi]\[CircleTimes]\[Theta]."
}], "Text",
